							<p>&nbsp;</p>
						</div>

					</div>
					<div id="tm-form-footer">
						<%= link_to 'Regresar', tipo_actividad_turisticas_path, class:"button back" %> |
						<%= link_to 'Modificar', edit_tipo_actividad_turistica_path(@tipo_actividad_turistica), class:"button edit" %>
