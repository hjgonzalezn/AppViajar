<% unless params[:id].blank? || params[:datos_control].blank? %>
	<% persona = Persona.where(pers_documentoIdentidad: "#{params[:id]}").take %>
	<% unless persona.nil? %>
		<% hshDatosControl = params[:datos_control] %>
		<% hshDatosControl.each do |key, value| %>
			<% case key %>
				<% when "nombre" %>
					$("#<%= value %>").val("<%= persona.pers_nombres + " " + persona.pers_apellidos %>");
			<% end %>
		<% end %>
	<% end %>
<% end %>
	